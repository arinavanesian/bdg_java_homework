package prim_data_types;

public class prim_data_types {
	public static void main(String[] args) {
		
		//GROUP INTEGER
		byte a = -128;
		byte b = 127;
		short c = -32768;
		short d = 32767;
		int e = -31;
		int f = 33;
		long g = 1232144325;
		long h = -1231234522;
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);
		System.out.println(g);
		System.out.println(h);
		System.out.println(Byte.MAX_VALUE);
		System.out.println(Byte.MIN_VALUE);
		System.out.println(Integer.MAX_VALUE);
		System.out.println(Integer.MIN_VALUE);
		System.out.println(Short.MAX_VALUE);
		System.out.println(Short.MIN_VALUE);
		System.out.println(Long.MAX_VALUE);
		System.out.println(Long.MIN_VALUE);
		//GROUP FLOAT
		float j = 23.4f;
		float k = 43.48888f;
		double l = 534.09d;
		double m =  11213.09d;
		System.out.println(j);
		System.out.println(k);
		System.out.println(l);
		System.out.println(m);
		System.out.println(Double.MAX_VALUE);
		System.out.println(Double.MIN_VALUE);
		System.out.println(Float.MAX_VALUE);
		System.out.println(Float.MIN_VALUE);
		char n = 'n';
		char o = 'o';
		char p = 77;
		char q = 66;
		System.out.println(n);
		System.out.println(o);
		System.out.println(p);
		System.out.println(q);
		char num9 = 57;
		System.out.println(num9);
		boolean s = true;
		boolean t = false;
		System.out.println(s);
		System.out.println(t);
//		int year = 201|;
//		System.out.println(year);
		int aa = 7;
		long bb = aa;
		System.out.println(aa);
		System.out.println(bb);
		int myInt = 7;
		double myDouble = myInt;
		System.out.println(myInt);
		System.out.println(myDouble);
		double dd = 9.78d;
		int ii = (int) dd;
		System.out.println(dd);
		System.out.println(ii);
		int ff = 9999;
		byte gg = (byte) ff;
		System.out.println(ff);
		System.out.println(gg);

//		long max = 3123456789;
//		// Either underscore or dot must be placed between digits
//		double d = 1000_.00;
////		since i can assign only one symbol or character in char, concatination is not possible
//		char i =’i’; char j=’j’; char ij = i +j;
//		According to the camelCase rule variable names  can't start but can finish with integers 
//		int 7a = 8;
//		
//		long h = h  + 8;
		int bn, bm = 11; 
		System.out.println(bn);
	}


}
